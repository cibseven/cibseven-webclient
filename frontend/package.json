{
  "name": "cibseven-components",
  "version": "2.1.4-dev.5",
  "type": "module",
  "license": "",
  "main": "./src/main.js",
  "style": "dist/cibseven-components.css",
  "files": [
    "dist/cibseven-components.css",
    "dist/cibseven-components.es.js",
    "dist/cibseven-components.umd.js",
    "src/themes"
  ],
  "exports": {
    ".": {
      "import": "./dist/cibseven-components.es.js",
      "require": "./dist/cibseven-components.umd.js"
    },
    "./themes/*": "./src/themes/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:library": "cross-env BUILD_MODE=library vite build",
    "build:library:watch": "cross-env BUILD_MODE=library vite build --watch",
    "preview": "vite preview",
    "test": "vitest run",
    "test:report": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest run --coverage --reporter=default --reporter=junit --outputFile=target/vitest-reports/TEST-front-end.xml",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest",
    "test:e2e:prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress open --e2e'",
    "test:e2e:playwright": "start-server-and-test preview http://localhost:5173 'playwright test'",
    "test:e2e:playwright:ui": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'playwright test --ui'",
    "test:e2e:playwright:debug": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'playwright test --debug'",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@bpmn-io/form-js": "1.15.2",
    "@cib/common-frontend": "1.0.3-dev.12",
    "@mdi/font": "7.4.47",
    "apexcharts": "4.7.0",
    "axios": "1.13.2",
    "bootstrap": "5.3.8",
    "bpm-sdk": "2.1.1",
    "bpmn-js": "18.3.2",
    "dmn-js": "17.2.0",
    "js-sha256": "0.11.0",
    "moment": "2.30.1",
    "platform": "1.3.6",
    "vue": "3.5.16",
    "vue-global-events": "3.0.1",
    "vue-i18n": "11.1.10",
    "vue-router": "4.5.1",
    "vue3-apexcharts": "1.8.0",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@playwright/test": "^1.57.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/eslint-plugin": "1.1.44",
    "@vue/compiler-dom": "3.5.16",
    "@vue/server-renderer": "3.5.16",
    "@vue/test-utils": "2.4.6",
    "cross-env": "7.0.3",
    "cypress": "15.8.2",
    "eslint": "9.39.1",
    "eslint-plugin-cypress": "5.2.1",
    "eslint-plugin-vue": "9.32.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "jsdom": "26.1.0",
    "sass": "1.89.0",
    "start-server-and-test": "2.0.11",
    "vite": "6.4.1",
    "vite-plugin-vue-devtools": "7.7.6",
    "vitest": "3.1.3"
  },
  "publishConfig": {
    "registry": "https://artifacts.cibseven.org/repository/npm-hosted/"
  }
}
