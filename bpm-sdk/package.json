{
  "name": "bpm-sdk",
  "version": "2.1.0-dev.0",
  "description": "Camunda BPM SDK extracted as a standalone library.",
  "main": "dist/bpm-sdk.umd.js",
  "module": "dist/bpm-sdk.es.js",
  "type": "module",
  "license": "Apache-2.0",
  "files": [
    "dist/bpm-sdk.es.js",
    "dist/bpm-sdk.umd.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/bpm-sdk.es.js",
      "require": "./dist/bpm-sdk.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:library": "cross-env BUILD_MODE=library vite build",
    "dev": "vite",
    "test": "vitest run",
    "bump:dev": "../bump-dev-version.sh bpm-sdk",
    "bump:dev:win": "../bump-dev-version.bat bpm-sdk",
    "remove:dev": "../npm-version-release.sh bpm-sdk",
    "remove:dev:win": "../npm-version-release.bat bpm-sdk"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.3",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "superagent": "^10.2.1"
  },
  "devDependencies": {
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "cross-env": "^7.0.3"
  },
  "publishConfig": {
    "registry": "https://artifacts.cibseven.org/repository/npm-hosted/"
  }
}
